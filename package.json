{
  "name": "scratchhw-link-desktop",
  "version": "0.2.1-beta",
  "description": "Scratch-HW link desktop",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && cpy ./ ../../../extensions --parents --cwd=./node_modules/scratchhw-extension/src",
    "cpext": "cpy ./ ../../../extensions --parents --cwd=./node_modules/scratchhw-extension/src",
    "start": "electron .",
    "build": "npm run build:pack",
    "build:pack": "electron-builder --dir",
    "build:dist": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScratchHW/scratchhw-link-desktop.git"
  },
  "keywords": [
    "scratchhw",
    "link",
    "scratch3.0",
    "desktop"
  ],
  "author": "ScratchHW Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ScratchHW/scratchhw-link-desktop/issues"
  },
  "homepage": "https://github.com/ScratchHW/scratchhw-link-desktop#readme",
  "dependencies": {
    "scratchhw-extension": "0.1.0-prerelease.20210114151218",
    "scratchhw-link": "0.1.0-prerelease.20210111072835"
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "rimraf": "^3.0.2"
  }
}
